    "react_component_type" : "AngieDefaultNode",
    "json_data_schema" : "{\n    \"description\": \"XML to JSON\",\n    \"type\": \"object\",\n    \"required\": [\n        \"label\",\n        \"endpoint_url\",\n        \"output_format\",\n        \"code\"\n    ],\n    \"properties\": {\n        \"label\": {\n            \"title\": \"Name\",\n            \"type\": \"string\"\n        },\n        \"endpoint_url\": {\n            \"title\": \"URL Endpoint\",\n            \"type\": \"string\"\n        },\n        \"soap_action\": {\n            \"title\": \"SOAP Action\",\n            \"type\": \"string\"\n        },\n        \"code\": {\n            \"title\": \"Mensaje\",\n            \"type\": \"string\"\n        },\n        \"cxf_extra_query_params\": {\n            \"title\": \"Opciones CXF\",\n            \"type\": \"array\",\n            \"items\": {\n                \"type\": \"object\",\n                \"required\": [\n                    \"code\",\n                    \"value\"\n                ],\n                \"properties\": {\n                    \"code\": {\n                        \"title\": \"Clave\",\n                        \"type\": \"string\"\n                    },\n                    \"value\": {\n                        \"title\": \"Valor\",\n                        \"type\": \"string\"\n                    }\n                }\n            }\n        }\n    }\n}",
    "json_ui_schema" : "{\n    \"endpoint_url\": {\n        \"ui:classNames\": \"col-12\"\n    },\n    \"soap_action\": {\n        \"ui:soap_action\": \"9\"\n    },\n    \"code\": {\n        \"ui:classNames\": \"col-12\",\n        \"ui:widget\": \"AceEditorWidget\",\n        \"ui:mode\": \"xml\",\n        \"ui:height\": \"200px\"\n    }\n}",
    "xml_template" : "<route id=\"{{source}}\">\n    <from uri=\"direct:{{source}}\" />\n    {{applyUnmarshal input_format input_format_options}}\n    <setBody>\n        <simple trim=\"false\">\n<![CDATA[{{safe code}}]]>\n        </simple>\n    </setBody>\n    {{#if (ne soap_action null)}}\n    <setHeader name=\"SOAPAction\">\n        <constant>\n            {{soap_action}}\n        </constant>\n    </setHeader>\n    {{/if}}\n    <to uri=\"cxf://{{endpoint_url}}{{querystring cxf_query_params cxf_extra_query_params}}\"/>\n    {{applyUnmarshal output_format output_format_options}}\n    {{applyMarshal output_format output_format_options}}\n    <multicast>\n        {{#each target}}\n        <to uri=\"direct:{{this}}\"/>\n        {{/each}}\n    </multicast>\n</route>",
    "defaults" : "{\n    \"cxf_query_params\": {\n        \"dataFormat\": \"RAW\"\n    },\n    \"input_format\": \"raw\",\n    \"output_format\": \"raw\"\n}",
